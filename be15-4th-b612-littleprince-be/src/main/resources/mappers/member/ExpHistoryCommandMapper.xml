<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="littleprince.member.command.mapper.ExpHistoryCommandMapper">

    <insert id="insertExpHistory">
        INSERT INTO exp_history (member_id, exp_point)
        VALUES (#{memberId}, #{expPoint})
    </insert>

    <insert id="insertExpHistoryId">
        INSERT INTO exp_history (member_id, exp_point, project_id)
        VALUES (#{memberId}, #{expPoint}, #{projectId})
    </insert>

</mapper>
